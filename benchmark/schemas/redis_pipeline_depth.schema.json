{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://rediscoro.dev/schemas/redis_pipeline_depth.schema.json",
  "title": "rediscoro redis_pipeline_depth benchmark report",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "schema_version",
    "timestamp_utc",
    "build_dir",
    "iterations",
    "warmup",
    "scenarios"
  ],
  "properties": {
    "schema_version": {
      "type": "integer",
      "const": 1
    },
    "timestamp_utc": {
      "type": "string",
      "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}Z$"
    },
    "build_dir": {
      "type": "string",
      "minLength": 1
    },
    "iterations": {
      "type": "integer",
      "minimum": 1
    },
    "warmup": {
      "type": "integer",
      "minimum": 0
    },
    "scenarios": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": [
          "sessions",
          "total_cmds_per_session",
          "pipeline_depth",
          "rediscoro_throughput_ops_s_runs",
          "rediscoro_p95_cmd_us_runs",
          "boostredis_throughput_ops_s_runs",
          "boostredis_p95_cmd_us_runs",
          "rediscoro_throughput_ops_s_median",
          "rediscoro_p95_cmd_us_median",
          "boostredis_throughput_ops_s_median",
          "boostredis_p95_cmd_us_median",
          "ratio_vs_boostredis_throughput"
        ],
        "properties": {
          "sessions": {
            "type": "integer",
            "minimum": 1
          },
          "total_cmds_per_session": {
            "type": "integer",
            "minimum": 1
          },
          "pipeline_depth": {
            "type": "integer",
            "minimum": 1
          },
          "rediscoro_throughput_ops_s_runs": {
            "type": "array",
            "minItems": 1,
            "items": {
              "type": "number",
              "exclusiveMinimum": 0
            }
          },
          "rediscoro_p95_cmd_us_runs": {
            "type": "array",
            "minItems": 1,
            "items": {
              "type": "number",
              "exclusiveMinimum": 0
            }
          },
          "boostredis_throughput_ops_s_runs": {
            "type": "array",
            "minItems": 1,
            "items": {
              "type": "number",
              "exclusiveMinimum": 0
            }
          },
          "boostredis_p95_cmd_us_runs": {
            "type": "array",
            "minItems": 1,
            "items": {
              "type": "number",
              "exclusiveMinimum": 0
            }
          },
          "rediscoro_throughput_ops_s_median": {
            "type": "number",
            "exclusiveMinimum": 0
          },
          "rediscoro_p95_cmd_us_median": {
            "type": "number",
            "exclusiveMinimum": 0
          },
          "boostredis_throughput_ops_s_median": {
            "type": "number",
            "exclusiveMinimum": 0
          },
          "boostredis_p95_cmd_us_median": {
            "type": "number",
            "exclusiveMinimum": 0
          },
          "ratio_vs_boostredis_throughput": {
            "type": "number",
            "minimum": 0
          }
        }
      }
    }
  }
}
